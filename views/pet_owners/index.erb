<h1> Owners and their pets</h1>


<% @pets.each do |pet| %>
<p> Name: <%= pet.name %></p>
<p> Breed: <%= pet.breed %> </p>
<p> Sex: <%= pet.gender %> </p>
<p> Adoptable: <%= pet.adoptable %> </p>
<p> Admission Date: <%= pet.date_arrived %> </p>
<p><a href="/pets/<%=pet.id%>">View</a></p>
<hr>

<% end %>
